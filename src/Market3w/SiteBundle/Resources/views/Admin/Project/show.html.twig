{% extends 'Market3wSiteBundle::layout.html.twig' %}

{% block content %}
    <div class="fixed_content">
    <h1>Project</h1>
    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Presentation</th>
                <td>{{ entity.presentation }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ entity.url }}</td>
            </tr>
            <tr>
                <th>Duty</th>
                <td>{{ entity.duty }}</td>
            </tr>
            <tr>
                <th>Work</th>
                <td>{{ entity.work }}</td>
            </tr>
            <tr>
                <th>Screenshot</th>
                <td>{{ entity.screenshot }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_project') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_project_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
</div>
{% endblock %}
