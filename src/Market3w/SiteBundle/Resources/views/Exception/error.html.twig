{% extends 'Market3wSiteBundle::layout.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets 
        'bundles/market3wsite/css/jquery-ui.min.css'
        'bundles/market3wsite/css/jquery-ui.structure.min.css' 
        output='jquery-ui.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts 
        'bundles/market3wsite/js/jquery-ui-1.11.0-custom.min.js'
        'bundles/market3wsite/js/appointment.js'
        output='appointment.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{% block content %}
        <h1>Une Erreur est survenue !</h1>
        <h2>The server returned a "{{ status_code }} {{ status_text }}".</h2>

        <div>
            Something is broken. Please e-mail us at [email] and let us know
            what you were doing when this error occurred. We will fix it as soon
            as possible. Sorry for any inconvenience caused.
        </div>
   {% endblock %}